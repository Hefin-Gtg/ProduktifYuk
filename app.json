{
  "expo": {
    "name": "ProduktifYuk",
    "slug": "ProduktifYuk",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon1.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.fnf.produktifyuk",
      "googleServicesFile": "./google-services/prod/GoogleService-Info.plist",
      "supportsTablet": true
    },
    "android": {
      "package": "com.fnf.produktifyuk",
      "googleServicesFile": "./google-services/prod/google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon1.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "9c7db171-8fec-426c-9957-bf11bfc16ea1"
      }
    }
  }
}
